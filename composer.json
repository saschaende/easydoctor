{
    "name": "saschaende/easydoctor",
    "description": "Fast and easy documentation generator for markdown files",
    "keywords": ["markdown", "documentation","generator","pdf","html","rst","latex","pandoc","automatically","website"],
    "homepage": "http://www.easydoctor.org",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SaschaEnde\\Easydoctor\\": "lib/"
        }
    },
    "authors": [
        {
            "name": "Sascha Ende",
            "email": "info@sascha-ende.de",
            "homepage": "http://www.sascha-ende.de"
        }
    ],
    "require": {
        "cagartner/phpquery": "^0.9.8",
        "erusev/parsedown-extra": "^0.7.1",
        "mpdf/mpdf": "^6.1",
        "saschaende/geshi": "^1.0"
    },
    "config": {
        "preferred-install": "source",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
