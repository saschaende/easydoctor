<office:text>
<text:list text:style-name="L1">
  <text:list-item>
    <text:p text:style-name="P1"><text:a xlink:type="simple" xlink:href="#page1" office:name=""><text:span text:style-name="Definition">Installation</text:span></text:a></text:p><text:list text:style-name="L2">
      <text:list-item>
