      </text:list-item>
      <text:list-item>
        <text:p text:style-name="P3"><text:a xlink:type="simple" xlink:href="#part4" office:name=""><text:span text:style-name="Definition">Konstanten
        für die SOLR Connection einrichten</text:span></text:a></text:p>
      </text:list-item>
